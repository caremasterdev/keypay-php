{
    "name": "healyhatman/keypay-php",
    "description": "Package to access the KeyPay API",
    "keywords": [
        "healyhatman",
        "laravel",
        "keypay-php"
    ],
    "homepage": "https://github.com/healyhatman/keypay-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamron Brooks",
            "email": "kamron@skyspider.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "healyhatman/oauth2-keypay" : "^1",
        "guzzlehttp/psr7": "^2.0",
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "KeypayPHP\\": "src/KeypayPHP/",
            "OpenAPI\\Client\\" : "src/lib/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}